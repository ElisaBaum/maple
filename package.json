{
  "name": "maple-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "migrate": "db-migrate up --config migrations-config.json -e db",
    "migrate-down": "db-migrate down -c 1 --config migrations-config.json -e db",
    "create-migration": "db-migrate create --config migrations-config.json -e db",
    "build": "tsc",
    "lint": "tslint ./lib/**/*.ts",
    "test": "mocha",
    "cover": "nyc mocha"
  },
  "nyc": {
    "lines": 50,
    "statements": 50,
    "functions": 50,
    "branches": 50,
    "include": [
      "lib"
    ],
    "exclude": [
      "lib/server.ts",
      "lib/injector.ts",
      "lib/**/*.spec.ts"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "sourceMap": true,
    "instrument": true,
    "cache": true,
    "all": true,
    "check-coverage": true,
    "report-dir": "./coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ElisaBaum/maple-backend.git"
  },
  "author": "Elisa Baum",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ElisaBaum/maple-backend/issues"
  },
  "homepage": "https://github.com/ElisaBaum/maple-backend#readme",
  "devDependencies": {
    "@types/chai": "^4.0.8",
    "@types/mocha": "^2.2.44",
    "@types/supertest": "^2.0.4",
    "chai": "^4.1.2",
    "db-migrate": "^0.10.0-beta.24",
    "db-migrate-pg": "^0.2.5",
    "mocha": "^4.0.1",
    "nyc": "^11.3.0",
    "source-map-support": "^0.5.0",
    "sqlite3": "^3.1.13",
    "supertest": "^3.0.0",
    "tslint": "^5.8.0"
  },
  "dependencies": {
    "@etianen/base-error": "^0.2.1",
    "@types/body-parser": "^1.16.7",
    "@types/express": "^4.0.39",
    "@types/http-status-codes": "^1.0.29",
    "@types/jsonwebtoken": "^7.2.3",
    "@types/multer": "^1.3.5",
    "@types/passport": "^0.3.5",
    "@types/passport-http": "^0.3.3",
    "@types/passport-jwt": "^3.0.0",
    "aws-sdk": "^2.175.0",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "di-typescript": "^1.1.4",
    "express": "^4.16.2",
    "http-status-codes": "^1.3.0",
    "jsonwebtoken": "^8.1.0",
    "multer": "^1.3.0",
    "passport": "^0.4.0",
    "passport-http": "^0.3.0",
    "passport-jwt": "^3.0.1",
    "pg": "^7.3.0",
    "reflect-metadata": "^0.1.10",
    "routing-controllers": "^0.7.6",
    "sequelize": "^4.20.1",
    "sequelize-typescript": "^0.6.1",
    "strong-error-handler": "^2.3.0",
    "ts-node": "^3.3.0",
    "typescript": "^2.5.3"
  }
}
